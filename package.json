{
  "name": "week-planner-card",
  "version": "1.4.0",
  "description": "Custom Home Assistant card to display events for a number of days from one or several calendars.",
  "source": "src/index.js",
  "module": "dist/week-planner-card.js",
  "targets": {
    "module": {
      "includeNodeModules": true,
      "optimize": true
    }
  },
  "scripts": {
    "start": "node src/index.js",
    "watch": "parcel watch",
    "build2": "parcel build --public-url /Volumes/config/www/community/week-planner-card/",
    "build": "parcel build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Smeltvig/week-planner-card.git"
  },
  "keywords": [
    "lovelace"
  ],
  "author": "Thomas",
  "licence": "MIT",
  "bugs": {
    "url": "https://github.com/Smeltvig/week-planner-card/issues"
  },
  "homepage": "https://github.com/Smeltvig/week-planner-card",
  "devDependencies": {
    "@parcel/optimizer-data-url": "^2.12.0",
    "@parcel/transformer-inline-string": "^2.11.0",
    "click-and-hold": "^2.0.0",
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "lit": "^3.1.2",
    "luxon": "^3.4.4",
    "uuid": "^10.0.0"
  }
}
